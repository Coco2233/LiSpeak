vai [a ](LINE site)
  goto "$site$"
  
<open,show> [my ]<documents,files>[ folder]
  open 'xdg-user-dir DOCUMENTS | xargs nautilus'
<open,show> [my ]music[ folder]
  open 'xdg-user-dir MUSIC | xargs nautilus'
<open,show> [my ]home[ folder]
  open 'xdg-user-dir | xargs nautilus'
<open,show> [my ]videos[ folder]
  open 'xdg-user-dir VIDEOS | xargs nautilus'
<open,show> [my ]pictures[ folder]
  open 'xdg-user-dir PICTURES | xargs nautilus'
<open,show> [my ]downloads[ folder]
  open 'xdg-user-dir DOWNLOAD | xargs nautilus'

open [google ]chrome
  open 'google-chrome'
  
open firefox
  open 'firefox'

[<show,display,what[ is]>][ the][ ]date[is it][ <today,[right ]now>]
  date date
[<show,display,what[ is]>][ the][ ]time[is it][ <today,[right ]now>]
