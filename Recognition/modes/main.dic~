play (LINE extra)
  mediaControl play
stop (LINE extra)
  mediaControl stop
next
  mediaControl next
prev
  mediaControl prev
<volume >mute< volume>
  mediaControl volume_mute
<<increase,raise,more,higher> volume,volume <up,increase,higher,raise>>
  mediaControl volume_up
<<decrease,lower,less,down> volume,volume <decrease,lower,less,down>>
  mediaControl volume_down



<say,speak> (LINE words)
  say "$words$"
<type,write> (LINE words)
  type "$words$\\r"
copy[ this]
  type '\\Cc'
paste[ this]
  type '\\Cv'

dictation mode
  mode dictation

<s,S>tatus
    result_from_call "git status"

go[ ]to (LINE site)
  goto "$site$"

open [google ]chrome
  open 'google-chrome'
open does not exit
  open 'bob'

